# EditorConfig helps maintain consistent coding styles
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
trim_trailing_whitespace = true

[*.yml]
indent_size = 2

[*.md]
indent_size = 2
max_line_length = off

[*.sol]
indent_size = 4

[*.ts]
indent_size = 2

[*.js]
indent_size = 2
