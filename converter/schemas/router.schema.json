{
"$schema": "https://json-schema.org/draft/2020-12/schema
",
"title": "AutoConverter Router Matrix (v1)",
"type": "object",
"properties": {
"version": { "type": "string", "const": "v1" },
"routes": {
"type": "array",
"items": {
"type": "object",
"properties": {
"from": { "type": "string", "pattern": "^0x[0-9a-fA-F]{40}$" },
"to": { "type": "string", "pattern": "^0x[0-9a-fA-F]{40}$" },
"symbolFrom": { "type": "string" },
"symbolTo": { "type": "string" },
"adapter": { "type": "string", "minLength": 2 },
"maxSlippageBps": { "type": "integer", "minimum": 0, "maximum": 10000 },
"minLiquidityUSD": { "type": "string", "pattern": "^[0-9]+$" },
"whitelisted": { "type": "boolean" }
},
"required": ["from","to","adapter","maxSlippageBps","minLiquidityUSD","whitelisted"],
"additionalProperties": false
}
}
},
"required": ["version","routes"],
"additionalProperties": false
}
