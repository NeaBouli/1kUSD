{
  "//": [
    "Linkcheck configuration for docs/.",
    "This file does NOT change CI by itself.",
    "It is intended to be used by the manual docs-linkcheck workflow later."
  ],
  "max_concurrency": 4,
  "max_retries": 2,
  "timeout": 10,
  "exclude": [
    "localhost",
    "127.0.0.1",
    "0.0.0.0",
    "example.com"
  ],
  "accept": [
    "200",
    "301",
    "302",
    "429"
  ],
  "user_agent": "1kUSD-docs-linkcheck",
  "cache": true
}
